{
  "manifest_version": 3,
  "name": "Send to Webhook",
  "version": "1.1.0",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "scripting",
    "notifications"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "/srcs/popup.html",
    "default_icon": "/images/icon.png"
  },
  "icons": {
    "16": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "dist/webhook_tools.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}